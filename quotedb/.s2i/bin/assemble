#!/bin/bash

set -o errexit
set -o nounset
set -o pipefail

shopt -s dotglob
echo "---> Installing application source $PWD ..."
#mv /tmp/src/* ./ 2>/dev/null || true
mv -v /tmp/src/* ./ 
WHERE=$(pwd)
echo "---> did move actually work!? $WHERE"

# Fix source directory permissions
/usr/libexec/fix-permissions ./

